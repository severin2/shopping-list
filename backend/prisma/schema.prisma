// Prisma schema for shopping-list backend

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Item {
  id          Int     @id @default(autoincrement())
  name        String
  description String
  quantity    Int     @default(1)
  isPurchased Boolean @default(false)
}
